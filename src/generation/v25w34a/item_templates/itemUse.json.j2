{
  "criteria": {
    "use_item": {
      "trigger": "minecraft:using_item",
      "conditions": {
        "item": {
          "predicates": {
            "minecraft:custom_data": {
              "{{ item }}": true
            }
          }
        }
      }
    }
  },
  "rewards": {
    "function": "{{ packNamespace }}:items/{{ item }}/{{ item }}"
  }
}
